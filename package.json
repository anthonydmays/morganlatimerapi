{
  "name": "orderprocessor",
  "version": "1.0.0",
  "description": "Order post-processor API for morganlatimer.com",
  "main": "bin/server.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "deploy": "npm run build && gcloud app deploy",
    "fix": "git-clang-format",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "node bin/server.js",
    "start:local": "npm run build && node bin/server.js",
    "test": "jasmine-ts --config=jasmine.json",
    "test:watch": "nodemon --ext ts --exec 'npm test'",
    "test:coverage": "nyc npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix",
      "pre-push": "npm run test:coverage"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.mock.ts",
      "**/*.d.ts",
      "types",
      "node_modules"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html",
      "cobertura",
      "text"
    ],
    "require": [
      "source-map-support/register"
    ],
    "sourceMap": true,
    "cache": false,
    "all": true
  },
  "author": "Anthony D. Mays",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.2.0",
    "@google-cloud/storage": "^2.5.0",
    "@types/body-parser": "^1.17.0",
    "@types/gapi": "0.0.36",
    "@types/gapi.auth2": "0.0.50",
    "@types/gapi.client.calendar": "^3.0.1",
    "@types/node": "^11.13.10",
    "@types/proxyquire": "^1.3.28",
    "express": "^4.16.4",
    "googleapis": "^27.0.0",
    "intuit-oauth": "^1.1.3",
    "karma-jspm": "^2.2.3",
    "node-quickbooks": "^2.0.31"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jasmine": "^3.3.12",
    "@types/supertest": "^2.0.7",
    "clang-format": "^1.2.4",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-es6-spies": "0.0.4",
    "jasmine-junit-reporter": "0.0.2",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "js-beautify": "^1.9.0-beta5",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.0.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.0.0",
    "prettier": "^1.16.4",
    "proxyquire": "^2.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
